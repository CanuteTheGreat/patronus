{
  "dashboard": {
    "title": "Patronus SD-WAN - Overview",
    "tags": ["patronus", "sdwan", "overview"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "System Uptime",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "system_uptime_seconds",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "s"
        }
      },
      {
        "id": 2,
        "title": "Total Sites",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sdwan_sites_total",
            "refId": "A"
          }
        ],
        "options": {
          "graph Mode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 3,
        "title": "Active Paths",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sdwan_paths_active",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 4,
        "title": "Active Users",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "active_users_total",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 5,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{path}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "HTTP Request Duration (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{path}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Path Latency Distribution",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(sdwan_path_latency_ms_bucket[5m])",
            "format": "heatmap",
            "refId": "A"
          }
        ],
        "yAxis": {"format": "ms"}
      },
      {
        "id": 8,
        "title": "Path Packet Loss",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sdwan_path_packet_loss_pct",
            "legendFormat": "{{path_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Packet Loss %"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "WebSocket Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "websocket_connections_active",
            "legendFormat": "Active connections",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Connections"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "Authentication Activity",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "rate(auth_login_success_total[5m])",
            "legendFormat": "Successful logins",
            "refId": "A"
          },
          {
            "expr": "rate(auth_login_failures_total[5m])",
            "legendFormat": "Failed logins",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Login Rate"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "title": "Database Query Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "p99 - {{type}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      }
    ]
  }
}
