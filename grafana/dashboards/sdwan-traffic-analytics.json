{
  "dashboard": {
    "title": "SD-WAN Traffic Analytics",
    "tags": ["patronus", "sdwan", "traffic", "analytics"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Active Flows",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 4, "h": 4},
        "targets": [{
          "expr": "patronus_active_flows_total"
        }],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Total Data Transferred",
        "type": "stat",
        "gridPos": {"x": 4, "y": 0, "w": 4, "h": 4},
        "targets": [{
          "expr": "sum(patronus_bytes_tx_total + patronus_bytes_rx_total)"
        }],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "decimals": 2
          }
        }
      },
      {
        "id": 3,
        "title": "Compression Ratio",
        "type": "stat",
        "gridPos": {"x": 8, "y": 0, "w": 4, "h": 4},
        "targets": [{
          "expr": "(1 - (sum(patronus_compression_output_bytes_total) / sum(patronus_compression_input_bytes_total))) * 100"
        }],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 10, "color": "yellow"},
                {"value": 30, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Bandwidth Saved (Compression)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 4, "h": 4},
        "targets": [{
          "expr": "sum(patronus_compression_input_bytes_total - patronus_compression_output_bytes_total)"
        }],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "decimals": 2
          }
        }
      },
      {
        "id": 5,
        "title": "Traffic by Protocol",
        "type": "piechart",
        "gridPos": {"x": 0, "y": 4, "w": 8, "h": 8},
        "targets": [{
          "expr": "sum by(protocol) (rate(patronus_bytes_tx_total[5m]))",
          "legendFormat": "{{protocol}}"
        }],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "donut"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "Bps"
          }
        }
      },
      {
        "id": 6,
        "title": "Traffic by Application",
        "type": "piechart",
        "gridPos": {"x": 8, "y": 4, "w": 8, "h": 8},
        "targets": [{
          "expr": "sum by(app_type) (rate(patronus_dpi_bytes_total[5m]))",
          "legendFormat": "{{app_type}}"
        }],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "donut"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "Bps"
          }
        }
      },
      {
        "id": 7,
        "title": "Top Flows by Bandwidth",
        "type": "table",
        "gridPos": {"x": 16, "y": 4, "w": 8, "h": 8},
        "targets": [{
          "expr": "topk(10, rate(patronus_flow_bytes_total[5m]))",
          "format": "table",
          "instant": true
        }],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true},
              "indexByName": {},
              "renameByName": {
                "src_ip": "Source",
                "dst_ip": "Destination",
                "protocol": "Protocol",
                "Value": "Bandwidth"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {"align": "auto"}
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Bandwidth"},
              "properties": [
                {"id": "unit", "value": "Bps"},
                {"id": "custom.align", "value": "right"}
              ]
            }
          ]
        }
      },
      {
        "id": 8,
        "title": "Traffic Rate by Path",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [{
          "expr": "rate(patronus_bytes_tx_total[5m]) * 8",
          "legendFormat": "{{path_id}}"
        }],
        "fieldConfig": {
          "defaults": {
            "unit": "bps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "stacking": {"mode": "normal"}
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Compression Efficiency Over Time",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [{
          "expr": "(1 - (rate(patronus_compression_output_bytes_total[5m]) / rate(patronus_compression_input_bytes_total[5m]))) * 100",
          "legendFormat": "Compression %"
        }],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 10, "color": "yellow"},
                {"value": 30, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "QoS Class Distribution",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(patronus_qos_bytes_total{class=\"realtime\"}[5m]) * 8",
            "legendFormat": "RealTime"
          },
          {
            "expr": "rate(patronus_qos_bytes_total{class=\"interactive\"}[5m]) * 8",
            "legendFormat": "Interactive"
          },
          {
            "expr": "rate(patronus_qos_bytes_total{class=\"streaming\"}[5m]) * 8",
            "legendFormat": "Streaming"
          },
          {
            "expr": "rate(patronus_qos_bytes_total{class=\"standard\"}[5m]) * 8",
            "legendFormat": "Standard"
          },
          {
            "expr": "rate(patronus_qos_bytes_total{class=\"bulk\"}[5m]) * 8",
            "legendFormat": "Bulk"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 30,
              "stacking": {"mode": "normal"}
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Policy Match Rate",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [{
          "expr": "rate(patronus_policy_matches_total[5m])",
          "legendFormat": "{{policy_name}}"
        }],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 12,
        "title": "DPI Classification Accuracy",
        "type": "gauge",
        "gridPos": {"x": 0, "y": 28, "w": 6, "h": 6},
        "targets": [{
          "expr": "(sum(patronus_dpi_classified_flows_total) / sum(patronus_dpi_total_flows_total)) * 100"
        }],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "Flows by QoS Class",
        "type": "bargauge",
        "gridPos": {"x": 6, "y": 28, "w": 6, "h": 6},
        "targets": [{
          "expr": "sum by(class) (patronus_qos_flows_total)",
          "legendFormat": "{{class}}"
        }],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 14,
        "title": "SLA Compliance",
        "type": "stat",
        "gridPos": {"x": 12, "y": 28, "w": 6, "h": 6},
        "targets": [{
          "expr": "(count(patronus_path_latency_sla_met == 1) / count(patronus_path_latency_sla_met)) * 100"
        }],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 95, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 15,
        "title": "Traffic Shaping Drop Rate",
        "type": "timeseries",
        "gridPos": {"x": 18, "y": 28, "w": 6, "h": 6},
        "targets": [{
          "expr": "rate(patronus_traffic_shaping_drops_total[5m])",
          "legendFormat": "{{class}}"
        }],
        "fieldConfig": {
          "defaults": {
            "unit": "pps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20
            }
          }
        }
      }
    ]
  }
}
